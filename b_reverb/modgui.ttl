@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<https://github.com/dcoredump/setBfreak/b_reverb>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-setbfreak-organ-reverb.html> ;
        modgui:stylesheet <modgui/stylesheet-setbfreak-organ-reverb.css> ;
        modgui:screenshot <modgui/screenshot-setbfreak-organ-reverb.png> ;
        modgui:thumbnail <modgui/thumbnail-setbfreak-organ-reverb.png> ;
        modgui:brand "dcoredump" ;
        modgui:label "setBfreak Organ Reverb" ;
        modgui:model "boxy" ;
        modgui:panel "2-knobs" ;
        modgui:color "wood0" ;
        modgui:knob "steel" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "mix" ;
            lv2:name "Dry/Wet" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "gain_in" ;
            lv2:name "Input Gain" ;
        ] ;
    ] .